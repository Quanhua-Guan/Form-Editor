{
  "name": "Form Editor",
  "version": "1.0",
  "description": "An extension for editing custom request(GET or POST) to web server.",
  "permissions": [ "tabs" ],
  "content_scripts": [
    {
      "matches": [ "http://*/*", "https://*/*" ],
      "js": ["form_discover.js"]
    }
  ],  
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }  
}

